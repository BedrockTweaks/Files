{
  "namespace": "bt_brewing_icons",

  "single_item_cell": {
    "type": "panel",
    "layer": 5,
    "$item_texture|default": "textures/misc/missing_texture",
    "$image_size|default": [16, 16],
    "size": [18, 18],
    "controls": [
      {
        "background": {
          "type": "image",
          "layer": 6,
          "texture": "textures/ui/cell_image_lock",
          "size": [18, 18],
          "offset": [0, 0],
          "anchor_from": "center",
          "anchor_to": "center"
        }
      },
      {
        "item@item_icon": {}
      },
      {
        "hover_button@hover_button": {}
      }
    ]
  },

  "multi_item_cell": {
    "type": "panel",
    "layer": 5,
    "size": [18, 108],
    "$background_size|default":[18, 108],
    "controls": [
      {
        "background": {
          "type": "image",
          "layer": 6,
          "texture": "textures/ui/cell_image_lock",
          "size": "$background_size",
          "offset": [0, 0],
          "anchor_from": "center",
          "anchor_to": "center"
        }
      }
    ]
  },

  "item_icon": {
    "type": "image",
    "layer": 10,
    "texture": "$item_texture",
    "size": "$image_size",
    "offset": [0, 0],
    "anchor_from": "center",
    "anchor_to": "center"
  },

  "multi_item_panel": {
    "type": "panel",
    "layer": 5,
    "$item_texture|default": "textures/misc/missing_texture",
    "$image_size|default": [16, 16],
    "size": [18, 18],
    "controls": [
      {
        "item@item_icon": {}
      },
      {
        "hover_button@hover_button": {}
      }
    ]
  },

  "hover_button@common.button": {
    "layer": 15,
    "$pressed_button_name": "by DrAv0011",
    "size": [18, 18],
    "sound_volume": 0.0,
    "default_control": "",
    "pressed_control": "",
    "controls": [
      {
        "hover@common.empty_panel": {
          "size": [18, 18],
          "controls": [
            { "hover_text_image@bt_brewing_icons.hover_text_image": {} },
            {
              "white_border@common.white_border_slot": {
                "layer": 2,
                "size": [18, 18]
              }
            }
          ]
        }
      }
    ]
  },

  "hover_text_image": {
    "type": "image",
    "allow_clipping": false,
    "alpha": 1.0,
    "texture": "textures/ui/purpleBorder",
    "size": ["100%c+8px", "100%c+8px"],
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "$hover_offset|default": ["-100%x", 0],
    "offset": "$hover_offset",
    "layer": 25,
    "controls": [
      {
        "hover_text_panel": {
          "type": "panel",
          "size": ["100%cm - 1px", 9],
          "controls": [
            {
              "hover_text@hover_text": {
                "text": "$hover_text"
              }
            }
          ]
        }
      }
    ]
  },

  "hover_text": {
    "type": "label",
    "offset": [-0.5, -1.5],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 2,
    "color": [1.0, 1.0, 1.0],
    "$hover_text|default": "Unset",
    "shadow": false
  },

  "left_arrow": {
    "type": "image",
    "layer": 10,
    "texture": "textures/ui/brewing_guide/left_arrow",
    "size": [9, 6],
    "offset": [16, -5],
    "anchor_from": "center",
    "anchor_to": "center"
  },
  "right_arrow": {
    "type": "image",
    "layer": 10,
    "texture": "textures/ui/brewing_guide/right_arrow",
    "size": [9, 6],
    "offset": [16, -3],
    "anchor_from": "center",
    "anchor_to": "center"
  },
  "down_arrow": {
    "type": "image",
    "layer": 10,
    "texture": "textures/ui/brewing_guide/down_arrow",
    "size": [6, 7],
    "offset": [1, 6],
    "anchor_from": "center",
    "anchor_to": "center"
  },
  "plus": {
    "type": "image",
    "layer": 20,
    "texture": "textures/ui/brewing_guide/plus",
    "size": [8, 8],
    "offset": [5, 0],
    "anchor_from": "center",
    "anchor_to": "center"
  },

  "ii@single_item_cell": {
    "$image_size": [8, 8],
    "$item_texture": "textures/ui/brewing_guide/ii",
    "$hover_text": "bt.bg.level_2.potion",
    "$hover_offset": [18, 0]
