{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.2.json",
	"author": "DrAv0011",
	"name": "Bedrock Tweaks - Gameplay Changes - Graves",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"gametests": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.6.0"
			},
			"clearFiles": {
				"runWith": "nodejs",
				"script": "./filters/clearFiles.js"
			}
		},
		"profiles": {
			"build": {
				"filters": [
					{
						"profile": "default"
					},
					{
						"filter": "clearFiles"
					}
				],
				"export": {
					"target": "exact",
					"bpPath": "./build/BP",
					"rpPath": "./build/RP"
				}
			},
			"default": {
				"export": {
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"filter": "gametests",
						"settings": {
							"buildOptions": {
								"bundle": true,
								"entryPoints": [
									"data/gametests/src/index.ts"
								],
								"format": "esm",
								"minify": false,
								"target": "es2020"
							},
							"manifest": "BP/manifest.json",
							"moduleUUID": "da5802ff-6c19-46d8-a27a-1f717580968b",
							"modules": [
								"@minecraft/server@1.11.0",
								"@minecraft/server-ui@1.1.0"
							],
							"outfile": "BP/scripts/bt/gc/graves/main.js"
						}
					}
				]
			}
		}
	}
}