{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.bp.villager.dead": {
      "states": {
        "default": {
          "transitions": [
            {
              "dead": "!query.is_alive"
            }
          ]
        },
        "dead": {
          "on_entry": [
            "/tellraw @a { \"rawtext\" : [ { \"text\" : \"A villager has died!\" } ] }"
          ]
        }
      }
    }
  }
}
